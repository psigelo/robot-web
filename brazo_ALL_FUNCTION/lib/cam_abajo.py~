import serial
import time

arduino = serial.Serial('/dev/ttyUSB0', 9600)
arduino.open()

time.sleep(0.3)
arduino.write('V')
arduino.write('V')
time.sleep(0.3)
arduino.write('V')
arduino.write('V')
time.sleep(0.3)
arduino.write('V')
arduino.write('V')
time.sleep(0.3)
arduino.write('V')
arduino.write('V')

arduino.close

